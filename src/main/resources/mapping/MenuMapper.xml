        <result column="createTime" property="createTime" />
        <result column="version" property="version" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, menuName, showIndex, parentId, treePath, menuStatus, createTime, version
    </sql>

</mapper>

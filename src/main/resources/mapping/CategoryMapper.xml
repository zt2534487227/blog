<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zt.blog.dao.CategoryDao">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zt.blog.model.Category">
        <id column="id" property="id" />
        <result column="cName" property="cName" />
        <result column="showIndex" property="showIndex" />
        <result column="categoryStatus" property="categoryStatus" />
        <result column="createTime" property="createTime" />
        <result column="version" property="version" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, cName, showIndex, categoryStatus, createTime, version
    </sql>

</mapper>
